{
  "name": "learnpod",
  "version": "1.0.0",
  "description": "a step-by-step walkthrough of front end dev tools",
  "dependencies": {
    "angular": "^1.6.4",
    "angular-resource": "^1.6.4",
    "angular-route": "^1.6.4"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "jasmine-core": "^2.6.1",
    "jshint": "^2.9.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31"
  },
  "scripts": {
    "karma": "./node_modules/.bin/karma start karma.conf.js",
    "lint": "./node_modules/.bin/jshint chapter9/src/js/*.js",
    "start": "python -m SimpleHTTPServer",
    "pretest": "npm run lint",
    "test": "npm run karma"
  },
  "jshintConfig": {
    "curly": true,
    "laxcomma": true,
    "nonbsp": true,
    "notypeof": true,
    "strict": "global",
    "undef": true,
    "unused": true,
    "devel": true,
    "predef": [
      "angular"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ntno/learnpod.git"
  },
  "keywords": [
    "tutorial",
    "angular",
    "jasmine",
    "karma"
  ],
  "author": "ntno",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ntno/learnpod/issues"
  },
  "homepage": "https://github.com/ntno/learnpod#readme"
}
